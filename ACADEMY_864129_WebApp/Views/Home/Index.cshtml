@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center" >
    <h5 class="display-4">Welcome to BigStore Data Analytics</h5>   
</div>
<div class="clearfix" style="min-height:50px"></div>
<div class="container">    
    <div class="row">        
        <div class="col-xl-12" style="min-height:350px">
            @await Html.PartialAsync("_AlertData")
        </div>
        <div class="col-xl-12" style="min-height:350px">
            @await Html.PartialAsync("_TelemetryData")
        </div>
        <div class="col-xl-12" style="min-height:350px">
            @await Html.PartialAsync("_CosmosTelemetryData")
        </div>
    </div>   
</div>


