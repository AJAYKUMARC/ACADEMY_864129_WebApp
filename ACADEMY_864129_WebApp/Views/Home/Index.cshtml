@model ACADEMY_864129_WebApp.Models.MasterData;
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome to BigStore Data Analytics</h1>
    @*<p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>*@
</div>

<div class="container">
    <div class="clearfix"></div>
    <div class="row">
        <div class="col-6">
            @await Html.PartialAsync("_AlertData", Model.AzureTableAlertData)
        </div>
        <div class="col-6">
            @await Html.PartialAsync("_TelemetryData", Model.AzureTableTelemetryData)
        </div>
        <div class="col-6">
            @await Html.PartialAsync("_CosmosTelemetryData", Model.AzureCosmosDBTelemetryData)
        </div>
    </div>   
</div>


